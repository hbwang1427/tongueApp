#holder {
    background-image: url('../img/back-hand-phone-left.png');
    width: 740px;
    height: 586px;
}

#twits {
  position: absolute;
  margin-left: 211px;
  margin-top: 71px;
  width: 208px;
  height: 363px;
  word-break: break-word;
}

#profile {
    height: 363px;
}

#logo {
    height: 100px;
}

#footer {
  font-family: centrale_sans_book, tahoma, arial, helvetica, sans-serif;
  font-size: 14px;
  font-stretch: normal;
  font-style: normal;
  font-variant: normal;
  font-weight: normal;
  color: white;
  background-image: linear-gradient(145deg, rgb(11, 34, 101), rgb(0, 57, 166));
  padding-bottom: 30px;
}

.p-footer {
    padding-top: 36px;
    padding-bottom: 43px;
}

.p-logo {
    -webkit-transform: translate3d(0,0,0);
    transform: translate3d(0,0,0);
    display: block;
    position: absolute;
    background: url('../img/logo-tiler-m.png') 0 0 repeat-y;
    z-index: 20;
    width: 140px;
    height: 140px;
    padding-top: 87px;
    margin-top: 20px;
    margin-left: 300px;
    -moz-box-shadow: 0 3px 8px 3px rgba(0,0,0,0.4);
    -webkit-box-shadow: 0 3px 8px 3px rgba(0,0,0,0.4);
    box-shadow: 0 3px 8px 3px rgba(0,0,0,0.4);
}

.p-logo:before {
    content: '';
    background: url('../img/sprite-header-rebrand.png') 0 0;
    position: absolute;
    left: 0;
    top: 0;
    width: 140px;
    height: 140px;
}

.p-logo-row  {
    height: 195px;
    background: url('../img/MH_PE2.jpg') 0 0;
    margin-bottom: 30px;
}

.p-logo .p-refresh {
  position: absolute;
  width: 100%;
  height: 87px;
  top: 0;
}
.p-logo span {
  position: relative;
  z-index: 1;
  display: table-cell;
  color: #fff;
  vertical-align: middle;
  word-break: break-word;
  font-size: 17px;
  height: 41px;
  padding: 6px 13px 6px 13px;
}
text.highcharts-title {
  font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;
}
.title {
  font-family: centrale_sans_xbold, tahoma, arial, helvetica, sans-serif;
  font-size: 60px;
  font-stretch: normal;
  font-style: normal;
  font-variant: normal;
  font-weight: normal;
  color:#FFF;
  margin-left:550px;
  padding-top:50px;
}

pre {
  white-space: pre-wrap;
  white-space: -moz-pre-wrap;
  white-space: -pre-wrap;
  white-space: -o-pre-wrap;
  word-wrap: break-word;
}
